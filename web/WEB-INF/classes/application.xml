<?xml version = "1.0" encoding = "ISO-8859-1"?><!DOCTYPE application SYSTEM "dtds/application.dtd"><application name="SmartFinance"> 	<!-- 	A default module for each entity is assumed with the	controllers on <default-module/>	-->	<default-module>		<!--controller name="Typical"/-->		<controller name="TypicalNotResetOnSave"/>		<!--controller name="UrlParameters"/--> <!-- TODO - Disables for now as it produces errors in some cases! -->	</default-module>	<!-- 	You can define additional modules, for example:	  	<module name="OnlyReadTeacher">		<model name="Teacher"/>		<controller name="Print"/>	</module>			-->	<module name="Customer">		<model name="Customer"/>		<controller name="CustomerController"/>	</module>	<module name="DailyExpire">		<controller name="DailyExpire"/>	</module></application>